       "<!DOCTYPE html>\n",
       "<html>\n",
       "<head>\n",
       "    <link rel=\"stylesheet\" type=\"text/css\" href=\"visual.css\">\n",
       "    <meta charset=\"UTF-8\">\n",
       "    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n",
       "</head>\n",
       "<body>\n",
       "    <div class=\"prop\" id=\"graphProperties\"> \n",
       "        <h1>Graph Properties</h1>\n",
       "        <form id=\"graphForm\">\n",
       "            <div class=\"form-row\">\n",
       "                <label for=\"seed\">Seed</label>\n",
       "                <input type=\"number\" id=\"seed\" name=\"seed\" value=\"0\" required>\n",
       "            </div>\n",
       "            <div class=\"form-row\">\n",
       "                <label for=\"nodes\">Nodes</label>\n",
       "                <input type=\"number\" id=\"nodes\" name=\"nodes\" min=\"1\" value=\"64\" required>\n",
       "            </div>\n",
       "            <div class=\"form-row\">\n",
       "                <label for=\"edges\">Edges</label>\n",
       "                <input type=\"number\" id=\"edges\" name=\"edges\" min=\"0\" value=\"256\" required>\n",
       "            </div>\n",
       "            <div class=\"form-row\">\n",
       "                <label for=\"density\">Density</label>\n",
       "                <input type=\"number\" id=\"density\" name=\"density\" value=\"0.05\" step=\"0.001\" min=\"0\" max=\"1\" required>\n",
       "            </div>\n",
       "            <div class=\"checkbox-row\">\n",
       "                <label class=\"custom-checkbox-container\">Planar\n",
       "                    <input type=\"checkbox\" id=\"planar\" name=\"planar\">\n",
       "                    <span class=\"checkmark\"></span>\n",
       "                </label>\n",
       "            </div>\n",
       "            <div class=\"checkbox-row\">\n",
       "                <label class=\"custom-checkbox-container\">Directed\n",
       "                    <input type=\"checkbox\" id=\"directed\" name=\"directed\">\n",
       "                    <span class=\"checkmark\"></span>\n",
       "                </label>\n",
       "            </div>\n",
       "            <div class=\"checkbox-row\">\n",
       "                <label class=\"custom-checkbox-container\">Connected\n",
       "                    <input type=\"checkbox\" id=\"Connected\" name=\"connected\" checked>\n",
       "                    <span class=\"checkmark\"></span>\n",
       "                </label>\n",
       "            </div>\n",
       "                <button type=\"submit\" id=\"submitBtn\" style=\" font-weight: bold; margin-left:-100px;margin-top:10px;text-align:center;\">Submit</button>\n",
       "                <button type=\"button\" id=\"confirmBtn\" style=\"font-weight:bold;display:none;margin-left:30px;margin-top:-18px;text-align: center;\">Confirm</button>\n",
       "            </form>\n",
       "    </div>\n",
       "    <script>\n",
       "        const form = document.getElementById('graphForm');\n",
       "        const submitBtn = document.getElementById('submitBtn');\n",
       "        const confirmBtn = document.getElementById('confirmBtn');\n",
       "\n",
       "        form.addEventListener('submit', function(event) {\n",
       "            event.preventDefault();\n",
       "            submitBtn.style.color = '#aaaaaa';\n",
       "            confirmBtn.style.display = 'block';\n",
       "        });\n",
       "        \n",
       "        confirmBtn.addEventListener('click', function() {\n",
       "            const nodes = document.getElementById('nodes').value;\n",
       "            const edges = document.getElementById('edges').value;\n",
       "            const density = document.getElementById('density').value;            \n",
       "            const seed = document.getElementById('seed').value;\n",
       "            const planar = document.getElementById('planar').checked ? 1 : 0;\n",
       "            const directed = document.getElementById('directed').checked ? 1 : 0\n",
       "            const connected = document.getElementById('Connected').checked ? 1 : 0;\n",
       "            confirmBtn.style.display = 'none';\n",
       "            submitBtn.style.color = '#000000';\n",
       "\n",
       "            // const jsonContent = JSON.stringify({\n",
       "            //     seed: seed,\n",
       "            //     nodes: nodes,\n",
       "            //     edges: edges,\n",
       "            //     density: density,\n",
       "            //     planar: planar,\n",
       "            //     directed: directed,\n",
       "            //     connected: connected\n",
       "            // }, null, 4);\n",
       "\n",
       "        });\n",
       "    </script>\n",
       "</body>\n",
       "</html>"
      ],
